      ("￪", "↑") ;
      ("￫", "→") ;
      ("￬", "↓") ;
      ("￭", "■") ;
      ("￮", "○")
    ]

  module M = Map.Make(struct
                      type t = string
                      let compare = String.compare
                    end)

  let map =
    let rec iter acc = function
      | [] -> acc
      | (k,v)::l -> iter (M.add k v acc) l
    in
    iter M.empty table

  let without w =
    Text.validate w;
    let remove_accents c =
      try M.find c map with
        | Not_found -> c
    in
    Text.map remove_accents w
]
